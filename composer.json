{
    "name": "diag/gluu-php-sdk",
    "description": "Gluu SDK",
    "authors": [
        {
            "name": "Toan Au",
            "email": "toan.au@diag.vn"
        }
    ],
    "require-dev": {
        "vimeo/psalm": "^4.24",
        "captainhook/captainhook": "^5.10",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9.5"

    },
    "psr-4": {
        "Gluu\\": "src/"
    },
    "require": {
        "php": " ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.4",
        "psr/log" : "*",
        "ext-json": "*",
        "psy/psysh": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Gluu\\": "src/"
        },
        "files": [
            "helper/helper.php",
            "bootstrap.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gluu\\Providers\\GluuProvider"
            ]
        }
    }
}
